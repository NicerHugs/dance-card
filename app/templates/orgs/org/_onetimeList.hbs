{{#unless parentEvent}}
  <h3>Your One Time Events</h3>
{{/unless}}

{{#unless models}}
  You have no one time events.
  <a href="#/orgs/{{orgUrlId}}/create-event">Click here to add a new event</a>
{{/unless}}

{{#each models}}
  <li class="{{attributes.orgUrlId}}-event">
    <a href="#/orgs/{{attributes.orgUrlId}}/{{id}}">{{attributes.name}}</a>
    {{#dateDisplay}}{{attributes.startDate}}{{/dateDisplay}}
    {{attributes.startTime}}-{{attributes.endTime}}
  </li>
{{/each}}
