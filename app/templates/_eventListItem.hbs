  <h5><a href="#/orgs/{{event.orgUrlId}}/{{event.objectId}}">{{event.name}}</a></h5>
  {{#if dancer}}
    {{#if attending}}
      You are attending this event <a href="#" class="unrsvp">cancel your RSVP</a>
    {{else}}
      <a href="#" class="rsvp">RSVP</a>
    {{/if}}
  {{/if}}
  {{#if owner}}
    <a href="#" class="delete-event">delete this event</a>
  {{/if}}
  {{#unless loggedIn}}
    <a href="#" class="rsvp">RSVP</a>
  {{/unless}}
  {{#dateDisplay}}{{event.startDate.iso}}{{/dateDisplay}} {{#time}}{{event.startTime}}{{/time}}
